dev data

docker run -d --name dev_database -e POSTGRES_USER=postgres -e POSTGRES_PASSWORD=root -e POSTGRES_DB=userdb -v dev_volume:/var/lib/postgresql/data -p 5434:5432 --hostname dev_db  postgres:16.4-alpine

pg_admin
